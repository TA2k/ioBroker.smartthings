    "loglevel": "info",
    "mode": "daemon",
    "type": "household",
    "compact": true,
    "connectionType": "cloud",
    "dataSource": "poll",
    "materialize": true,
    "dependencies": [
      {
        "js-controller": ">=3.0.0"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=4.0.9"
      }
    ]
  },
  "encryptedNative": ["token"],
  "protectedNative": ["token"],
  "native": {
    "token": "",
    "interval": 10,
    "virtualInterval": 0,
    "exclude": "Cycle",
    "excludeDevices": ""
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Device or service connected",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
